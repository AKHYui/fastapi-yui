# /.env.example
# SQLAlchemy Database URL for MySQL 5.7
# Format: mysql+mysqlconnector://<user>:<password>@<host>:<port>/<dbname>
DATABASE_URL="mysql+mysqlconnector://root:your_password@localhost:3306/fastapi_db"

# JWT Settings
# To generate a new secret key, run: openssl rand -hex 32
SECRET_KEY="a_very_secret_key_that_you_must_change"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30